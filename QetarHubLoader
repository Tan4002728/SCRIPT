-- Hiá»‡u á»©ng cháº¡y chá»¯ cho title

spawn(function()

    local originalText = "QETAR HUB"

    local colors = {

        Color3.fromRGB(0, 170, 255),

        Color3.fromRGB(0, 255, 255),

        Color3.fromRGB(0, 200, 255),

        Color3.fromRGB(0, 150, 255)

    }

    

    while true do

        for i = 1, #colors do

            if Title and Title.Parent then

                TweenService:Create(Title, TweenInfo.new(0.5, Enum.EasingStyle.Sine), {

                    TextColor3 = colors[i]

                }):Play()

            end

            wait(0.8)

        end

    end

end)
