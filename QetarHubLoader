-- LOGIC VARIABLES

local JumpConnection = nil

local function UpdateSpeed()

    if SpeedEnabled and player.Character and player.Character:FindFirstChild("Humanoid") then

        player.Character.Humanoid.WalkSpeed = SpeedValue

    end

end

SpeedInputBox.FocusLost:Connect(function(enterPressed)

    local text = SpeedInputBox.Text

    local num = tonumber(text)

    

    if num then

        SpeedValue = math.clamp(num, 16, 300)

        SpeedInputBox.Text = tostring(SpeedValue)

        UpdateSpeed()

    else

        SpeedInputBox.Text = tostring(SpeedValue)

    end

end)

-- Speed Hack Logic vá»›i hiá»‡u á»©ng

SpeedToggle.MouseButton1Click:Connect(function()

    SpeedEnabled = not SpeedEnabled

    

    if SpeedEnabled then

        TweenService:Create(SpeedToggle, TweenInfo.new(0.3), {

            BackgroundColor3 = Color3.fromRGB(30, 100, 30),

            TextColor3 = Color3.fromRGB(0, 255, 0)

        }):Play()

        TweenService:Create(SpeedStroke, TweenInfo.new(0.3), {

            Color = Color3.fromRGB(0, 255, 0),

            Thickness = 2

        }):Play()

        SpeedToggle.Text = "ON"

        UpdateSpeed()

        

        player.CharacterAdded:Connect(function(char)

            task.wait(0.5)

            if char:FindFirstChild("Humanoid") and SpeedEnabled then

                char.Humanoid.WalkSpeed = SpeedValue

            end

        end)

    else

        TweenService:Create(SpeedToggle, TweenInfo.new(0.3), {

            BackgroundColor3 = Color3.fromRGB(50, 50, 50),

            TextColor3 = Color3.fromRGB(255, 50, 50)

        }):Play()

        TweenService:Create(SpeedStroke, TweenInfo.new(0.3), {

            Color = Color3.fromRGB(0, 170, 255),

            Thickness = 1

        }):Play()

        SpeedToggle.Text = "OFF"

        

        if player.Character and player.Character:FindFirstChild("Humanoid") then

            player.Character.Humanoid.WalkSpeed = 16

        end

    end

end)

-- Infinite Jump Logic vá»›i hiá»‡u á»©ng

JumpToggle.MouseButton1Click:Connect(function()

    JumpEnabled = not JumpEnabled

    

    if JumpEnabled then

        TweenService:Create(JumpToggle, TweenInfo.new(0.3), {

            BackgroundColor3 = Color3.fromRGB(30, 100, 30),

            TextColor3 = Color3.fromRGB(0, 255, 0)

        }):Play()

        TweenService:Create(JumpStroke, TweenInfo.new(0.3), {

            Color = Color3.fromRGB(0, 255, 0),

            Thickness = 2

        }):Play()

        JumpToggle.Text = "ON"

        

        -- Infinite Jump Logic

        JumpConnection = UserInputService.JumpRequest:Connect(function()

            if player.Character and player.Character:FindFirstChild("Humanoid") then

                player.Character.Humanoid:ChangeState("Jumping")

            end

        end)

    else

        TweenService:Create(JumpToggle, TweenInfo.new(0.3), {

            BackgroundColor3 = Color3.fromRGB(50, 50, 50),

            TextColor3 = Color3.fromRGB(255, 50, 50)

        }):Play()

        TweenService:Create(JumpStroke, TweenInfo.new(0.3), {

            Color = Color3.fromRGB(0, 170, 255),

            Thickness = 1

        }):Play()

        JumpToggle.Text = "OFF"

        

        if JumpConnection then

            JumpConnection:Disconnect()

            JumpConnection = nil

        end

    end

end)

-- ESP Logic vá»›i hiá»‡u á»©ng

ESPToggle.MouseButton1Click:Connect(function()

    ESPEnabled = not ESPEnabled

    

    if ESPEnabled then

        TweenService:Create(ESPToggle, TweenInfo.new(0.3), {

            BackgroundColor3 = Color3.fromRGB(30, 100, 30),

            TextColor3 = Color3.fromRGB(0, 255, 0)

        }):Play()

        TweenService:Create(ESPStroke, TweenInfo.new(0.3), {

            Color = Color3.fromRGB(0, 255, 0),

            Thickness = 2

        }):Play()

        ESPToggle.Text = "ON"

        

        -- ESP Logic

        local function AddESP(character)

            if character:FindFirstChild("HumanoidRootPart") then

                local highlight = Instance.new("Highlight")

                highlight.Name = "QetarESP"

                highlight.FillColor = Color3.fromRGB(255, 0, 0)

                highlight.FillTransparency = 0.8

                highlight.OutlineColor = Color3.fromRGB(255, 255, 0)

                highlight.OutlineTransparency = 0

                highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop

                highlight.Parent = character

                ESPHighlights[character] = highlight

                

                -- ThĂªm tĂªn ngÆ°á»i chÆ¡i

                local playerName = Players:GetPlayerFromCharacter(character)

                if playerName then

                    local billboard = Instance.new("BillboardGui")

                    billboard.Name = "ESPName"

                    billboard.Size = UDim2.new(0, 200, 0, 50)

                    billboard.StudsOffset = Vector3.new(0, 3, 0)

                    billboard.AlwaysOnTop = true

                    billboard.Adornee = character:WaitForChild("Head")

                    

                    local nameLabel = Instance.new("TextLabel")

                    nameLabel.Size = UDim2.new(1, 0, 1, 0)

                    nameLabel.BackgroundTransparency = 1

                    nameLabel.Text = playerName.Name

                    nameLabel.TextColor3 = Color3.fromRGB(0, 255, 255)

                    nameLabel.TextSize = 14

                    nameLabel.Font = Enum.Font.Code

                    nameLabel.Parent = billboard

                    

                    billboard.Parent = character

                end

            end

        end

        

        -- Ăp dá»¥ng ESP cho táº¥t cáº£ ngÆ°á»i chÆ¡i hiá»‡n táº¡i

        for _, otherPlayer in ipairs(Players:GetPlayers()) do

            if otherPlayer ~= player and otherPlayer.Character then

                AddESP(otherPlayer.Character)

            end

        end

        

        -- Theo dĂµi ngÆ°á»i chÆ¡i má»›i

        Players.PlayerAdded:Connect(function(otherPlayer)

            otherPlayer.CharacterAdded:Connect(function(character)

                if ESPEnabled then

                    AddESP(character)

                end

            end)

        end)

    else

        TweenService:Create(ESPToggle, TweenInfo.new(0.3), {

            BackgroundColor3 = Color3.fromRGB(50, 50, 50),

            TextColor3 = Color3.fromRGB(255, 50, 50)

        }):Play()

        TweenService:Create(ESPStroke, TweenInfo.new(0.3), {

            Color = Color3.fromRGB(0, 170, 255),

            Thickness = 1

        }):Play()

        ESPToggle.Text = "OFF"

        

        -- XĂ³a ESP

        for character, highlight in pairs(ESPHighlights) do

            highlight:Destroy()

            local billboard = character:FindFirstChild("ESPName")

            if billboard then

                billboard:Destroy()

            end

        end

        ESPHighlights = {}

    end

end)

-- Xá»­ lĂ½ sá»± kiá»‡n khi Character cháº¿t

player.CharacterAdded:Connect(function(character)

    character:WaitForChild("Humanoid").Died:Connect(function()

        if SpeedEnabled then

            task.wait(3) -- Äá»£i respawn

            if character:FindFirstChild("Humanoid") then

                character.Humanoid.WalkSpeed = SpeedValue

            end

        end

    end)

end)

-- DRAGGABLE GUI vá»›i hiá»‡u á»©ng

local dragging

local dragInput

local dragStart

local startPos

local function update(input)

    local delta = input.Position - dragStart

    MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)

end

TopBar.InputBegan:Connect(function(input)

    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then

        dragging = true

        dragStart = input.Position

        startPos = MainFrame.Position

        

        -- Hiá»‡u á»©ng khi kĂ©o

        TweenService:Create(MainFrameGlow, TweenInfo.new(0.2), {

            Thickness = 4,

            Color = Color3.fromRGB(0, 255, 255)

        }):Play()

        

        input.Changed:Connect(function()

            if input.UserInputState == Enum.UserInputState.End then

                dragging = false

                TweenService:Create(MainFrameGlow, TweenInfo.new(0.2), {

                    Thickness = 2,

                    Color = Color3.fromRGB(0, 170, 255)

                }):Play()

            end

        end)

    end

end)

TopBar.InputChanged:Connect(function(input)

    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then

        dragInput = input

    end

end)

UserInputService.InputChanged:Connect(function(input)

    if input == dragInput and dragging then

        update(input)

    end

end)

-- MINUS BUTTON (Minimize) vá»›i hiá»‡u á»©ng

local isMinimized = false

MinusButton.MouseButton1Click:Connect(function()

    isMinimized = not isMinimized

    

    if isMinimized then

        TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {

            Size = UDim2.new(0, 450, 0, 35)

        }):Play()

        TweenService:Create(MinusButton, TweenInfo.new(0.2), {

            TextColor3 = Color3.fromRGB(0, 255, 0)

        }):Play()

        MinusButton.Text = "+"

    else

        TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {

            Size = UDim2.new(0, 450, 0, 400)

        }):Play()

        TweenService:Create(MinusButton, TweenInfo.new(0.2), {

            TextColor3 = Color3.fromRGB(255, 255, 255)

        }):Play()

        MinusButton.Text = "-"

    end

end)

-- CLOSE BUTTON vá»›i hiá»‡u á»©ng

CloseButton.MouseButton1Click:Connect(function()

    TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {

        Size = UDim2.new(0, 0, 0, 0),

        BackgroundTransparency = 1

    }):Play()

    TweenService:Create(MainFrameGlow, TweenInfo.new(0.3), {

        Thickness = 0

    }):Play()

    

    wait(0.3)

    ScreenGui:Destroy()

end)

-- KEYBIND F9 Ä‘á»ƒ áº©n/hiá»‡n GUI

local isGUIVisible = true

UserInputService.InputBegan:Connect(function(input, gameProcessed)

    if gameProcessed then return end

    

    if input.KeyCode == Enum.KeyCode.F9 then

        isGUIVisible = not isGUIVisible

        

        if isGUIVisible then

            MainFrame.Visible = true

            TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {

                Size = UDim2.new(0, 450, 0, 400),

                BackgroundTransparency = 0

            }):Play()

        else

            TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {

                Size = UDim2.new(0, 0, 0, 0),

                BackgroundTransparency = 1

            }):Play()

            wait(0.3)

            MainFrame.Visible = false

        end

    end

end)

-- Tá»° Äá»˜NG Cáº¬P NHáº¬T WALKSPEED KHI RESPAWN

player.CharacterAdded:Connect(function(char)

    char:WaitForChild("Humanoid")

    

    if SpeedEnabled then

        char.Humanoid.WalkSpeed = SpeedValue

    end

    

    -- Ná»‘i láº¡i ESP náº¿u cáº§n

    if ESPEnabled then

        task.wait(1)

        for _, otherPlayer in ipairs(Players:GetPlayers()) do

            if otherPlayer ~= player and otherPlayer.Character and not ESPHighlights[otherPlayer.Character] then

                local highlight = Instance.new("Highlight")

                highlight.Name = "QetarESP"

                highlight.FillColor = Color3.fromRGB(255, 0, 0)

                highlight.FillTransparency = 0.8

                highlight.OutlineColor = Color3.fromRGB(255, 255, 0)

                highlight.OutlineTransparency = 0

                highlight.Parent = otherPlayer.Character

                ESPHighlights[otherPlayer.Character] = highlight

            end

        end

    end

end)

